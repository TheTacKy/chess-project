{
  "name": "chess-project",
  "version": "1.0.0",
  "description": "A multiplayer online chess game built with React, Express, and Socket.io",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "chess",
    "multiplayer",
    "socket.io",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC"
}



